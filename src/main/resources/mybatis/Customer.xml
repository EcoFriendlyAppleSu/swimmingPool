<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="swimming.pool.infra.mybatis.CustomerMapper">
  <insert id="insert" parameterType="swimming.pool.infra.mybatis.mappermodel.CustomerInfoDao"
    useGeneratedKeys="true" keyProperty="customerId">
    INSERT
    INTO customer(name, age)
    VALUES (#{name}, #{age})
  </insert>
</mapper>
